{
  "name": "electronome",
  "version": "0.0.4",
  "main": "main.js",
  "scripts": {
    "prestart": "[ -d gen ] || mkdir gen; npx jison parser.jison; mv parser.js gen",
    "start": "electron ."
  },
  "description": "programmable metronome",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jrkatz/electronome.git"
  },
  "keywords": [
    "programmable",
    "metronome",
    "electron",
    "electronome"
  ],
  "author": "Jacob Katz",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/jrkatz/electronome/issues"
  },
  "homepage": "https://github.com/jrkatz/electronome#readme",
  "devDependencies": {
    "electron": "^11.0.3",
    "eslint": "^7.14.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^4.3.0",
    "jison": "^0.4.18",
    "lint-staged": "^10.5.2",
    "prettier": "2.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "**/*": "prettier --write --ignore-unknown"
  }
}
